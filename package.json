{
  "name": "fse-project-backend",
  "version": "1.0.0",
  "description": "The backend for our FSE project",
  "main": "index.ts",
  "scripts": {
    "test": "npm run seed:undo && npx jest --silent=false --forceExit",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:generate": "npx sequelize-cli migration:generate --name",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed 20250419135728-seed-users.js",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:undo": "npx sequelize-cli db:seed:undo 20250419135728-seed-users.js",
    "dev": "tsx watch index.ts"
  },
  "keywords": [
    "fse",
    "project",
    "typescript",
    "backend",
    "sql",
    "api"
  ],
  "author": "fasihh",
  "license": "ISC",
